{
    "name": "neurony/laravel-duplicate",
    "description": "Duplicate any Eloquent model along with its underlying relationships",
    "license": "MIT",
    "keywords": [
        "laravel",
        "duplicate",
        "replicate",
        "copy",
        "eloquent",
        "model",
        "relations"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Andrei Badea",
            "email": "zbiller@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/contracts": "^7.0|^8.0|^9.0",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "illuminate/database": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Neurony\\Duplicate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Neurony\\Duplicate\\Tests\\": "tests"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
